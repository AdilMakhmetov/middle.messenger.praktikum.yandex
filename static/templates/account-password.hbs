<div class="container">
  <div class="actions">
    <button class="back">
      <img src="{{back}}" alt="">
    </button>
    <button class="cirleIcon cirleIcon-account">
      <img src="{{account}}" alt="">
    </button>
    <button class="cirleIcon cirleIcon-upload">
      <img src="{{upload}}" alt="">
    </button>
  </div>
  <div class="account">
    <h1 class="account--title">Смена пароля</h1>
    <h2 class="account--fullname">Jason Bourne</h2>
    <div class="inputs">
      <input type="password" class="input" placeholder="Пароль">
      <input type="password" class="input" placeholder="Пароль (еще раз)">
    </div>
    <button class="button button-primary button-full">
      <img src="{{password}}" alt="">
      <span class="button--text">Сменить пароль</span>
    </button>
  </div>
</div>
