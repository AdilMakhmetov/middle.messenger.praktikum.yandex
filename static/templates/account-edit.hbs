<div class="container">
  <div class="actions">
    <button class="back">
      <img src="{{back}}" alt="">
    </button>
    <button class="cirleIcon cirleIcon-account">
      <img src="{{account}}" alt="">
    </button>
    <button class="cirleIcon cirleIcon-upload">
      <img src="{{upload}}" alt="">
    </button>
  </div>
  <div class="account">
    <h1 class="account--title">Редактирование</h1>
    <h2 class="account--fullname">Jason Bourne</h2>
    <div class="inputs">
      <input type="email" class="input" placeholder="Почта">
      <input type="text" class="input" placeholder="Логин">
      <input type="text" class="input" placeholder="Имя">
      <input type="text" class="input" placeholder="Фамилия">
      <input type="text" class="input" placeholder="Имя в чате">
      <input type="tel" class="input" placeholder="Телефон">
    </div>
    <button class="button button-primary button-full">
      <img src="{{save}}" alt="">
      <span class="button--text">Сохранить</span>
    </button>
  </div>
</div>
